{
  "name": "@maxmorozoff/try-catch-tuple",
  "description": "A TypeScript utility function for structured error handling in both synchronous and asynchronous operations.",
  "version": "0.0.2",
  "license": "MIT",
  "author": "Max Morozov <max@morozov.page",
  "homepage": "https://github.com/maxmorozoff/try-catch-tuple#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxmorozoff/try-catch-tuple.git"
  },
  "keywords": [
    "tryCatchTuple",
    "tryCatch",
    "tuple",
    "utils",
    "utility",
    "utilities",
    "bun",
    "typescript"
  ],
  "scripts": {
    "test:unit": "bun test --coverage",
    "test:types": "tsc --noEmit",
    "test": "bun run test:unit && bun run test:types",
    "build": "tsup index.ts --format esm,cjs --sourcemap",
    "version": "changeset version && biome format --fix package.json",
    "lint": "biome check "
  },
  "type": "module",
  "module": "dist/index.js",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/*.js",
    "dist/*.cjs",
    "dist/*js.map",
    "dist/*.d.ts"
  ],
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@changesets/cli": "^2.28.1",
    "@types/bun": "^1.2.5",
    "tsup": "^8.4.0"
  },
  "peerDependencies": {
    "typescript": "^5.8.2"
  }
}
